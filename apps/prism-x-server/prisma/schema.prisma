datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt    DateTime @default(now())
  email        String?  @unique
  events       Json?
  firstName    String?
  id           String   @id @default(cuid())
  lastName     String?
  password     String
  roles        Json
  updatedAt    DateTime @updatedAt
  username     String   @unique
  userProfiles Json?
}
